{
  "name": "frogbytes",
  "version": "1.0.0",
  "description": "Multifunctional Lecture Summarization & Learning Platform",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --max-warnings 0",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:unit": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:watch": "vitest",
    "test:ci": "vitest run --coverage && playwright test",
    "db:generate-types": "npx supabase gen types typescript --local > lib/supabase/database.types.ts",
    "db:reset": "npx supabase db reset",
    "db:push": "npx supabase db push",
    "db:start": "npx supabase start",
    "db:stop": "npx supabase stop",
    "security-audit": "npm audit --audit-level=moderate",
    "bundle-analyzer": "cross-env ANALYZE=true npm run build",
    "check-deps": "madge --circular .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "pre-commit": "lint-staged",
    "prepare": "husky install || true"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.45.4",
    "@types/archiver": "^6.0.3",
    "@types/recharts": "^1.8.29",
    "archiver": "^7.0.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "form-data": "^4.0.4",
    "framer-motion": "^12.23.24",
    "highlight.js": "^11.11.1",
    "lucide-react": "^0.544.0",
    "next": "^15.0.3",
    "node-latex": "^3.1.0",
    "pdfjs-dist": "^5.4.149",
    "puppeteer": "^24.24.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.6.0",
    "react-markdown": "^10.1.0",
    "react-pdf": "^10.1.0",
    "recharts": "^3.2.1",
    "redis": "^5.8.3",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "telegraf": "^4.16.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.1",
    "@testing-library/jest-dom": "^6.9.0",
    "@testing-library/react": "^16.3.0",
    "@types/crypto-js": "^4.2.2",
    "@types/form-data": "^2.2.1",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.7.9",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.0.3",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^14.0.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "supabase": "^2.48.3",
    "tailwindcss": "^3.4.14",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "ts-prune": "^0.10.3",
    "typescript": "5.9.3",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
