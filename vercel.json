{
  "crons": [
    {
      "path": "/api/cron/validate-keys",
      "schedule": "* * * * *" // run every minute to process pending validation continuously
    },
    {
      "path": "/api/cron/revalidate-keys",
      "schedule": "*/5 * * * *" // run every 5 minutes to revalidate working keys
    },
    {
      "path": "/api/cron/scrape-keys",
      "schedule": "0 * * * *" // run hourly
    }
  ]
}
